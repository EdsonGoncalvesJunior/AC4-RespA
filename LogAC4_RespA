def calc_media(peso,idade,n):
        cont=0
        soma_peso=0
        i=0
        while i<n:
                if idade[i]>60:
                        cont=cont+1
                        soma_peso=peso[i]+soma_peso
                i=i+1
        return soma_peso/cont

peso=[143,114,107,111,110,400,149,119,115,133,127,58,96,55,139,105,127,127,70,67,132,98,95,76,90,90,112,120,134,101,116,84,135,105,137,70,99,124,92,81,91,80,131,122,110,68,117,121,65,138]
idade=[37,60,40,89,79,96,57,49,95,79,40,57,55,97,95,55,74,81,45,44,44,98,47,38,42,84,37,83,84,82,83,52,42,63,80,40,58,77,83,71,78,53,95,44,48,64,32,37,91,67]


x= calc_media(peso,idade,50)
x = round (x,2)
print (x)
